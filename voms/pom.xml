			<groupId>external.voms</groupId>
			<artifactId>fnal-vox-vomrs-client</artifactId>
			<version>1.3.0</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>external.voms</groupId>
			<artifactId>glite-security-util-java</artifactId>
			<version>0.1</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>external.voms</groupId>
			<artifactId>glite-security-trustmanager</artifactId>
			<version>0.1</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>org.cogkit</groupId>
