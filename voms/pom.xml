<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>grix</artifactId>
    <groupId>au.org.arcs.grix</groupId>
    <version>1.1</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>au.org.arcs.grix</groupId>
  <artifactId>voms</artifactId>
  <version>1.1</version>
  	<repositories>
		<repository>
			<id>VPAC maven repository</id>
			<url>http://hudson.vpac.org/maven_repository</url>
		</repository>
	</repositories>

	<dependencies>
		<dependency>
			<groupId>au.org.arcs.grix</groupId>
			<artifactId>common</artifactId>
			<version>1.1</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>au.org.arcs.grix</groupId>
			<artifactId>qc</artifactId>
			<version>1.1</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.14</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>external.voms</groupId>
			<artifactId>axis</artifactId>
			<version>1.1</version>
			<scope>compile</scope>
